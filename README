## Initialize
dagger init --sdk=java --source=./ --name=java-build
dagger develop --sdk=java --source=./

dagger init --sdk=typescript --source=./ --name=java-build
dagger develop --sdk=typescript --source=./

dagger init --sdk=go --source=./ --name=java-build
dagger develop --sdk=go --source=./

## How to Use

dagger -m ../dagger-modules/java/java-build call publish --source=./ --repository ttl.sh --appName spring-petclinic --version 1.0.0
export JENKINS=jenkins && dagger -m ../dagger-modules/java/java-build call publish --source=./ --repository nexus/docker --username jenkins --password env:JENKINS --appName spring-petclinic --version 1.0.0

dagger -m ../dagger-modules/typescript/java-build call publish --source=./ --repository ttl.sh --appName spring-petclinic --version 1.0.0
export JENKINS=jenkins && dagger -m ../dagger-modules/typescript/java-build call publish --source=./ --repository nexus/docker --username jenkins --password env:JENKINS --appName spring-petclinic --version 1.0.0

dagger -m ../dagger-modules/go/java-build call publish --source=./ --repository ttl.sh --appName spring-petclinic --version 1.0.0
export JENKINS=jenkins && dagger -m ../dagger-modules/go/java-build call publishWithAuth --source=./ --repository nexus/docker --username jenkins --password env:JENKINS --appName spring-petclinic --version 1.0.0

